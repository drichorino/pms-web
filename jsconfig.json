{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~/*": [
                "./*"
            ],
            "@/*": [
                "./*"
            ],
            "~~/*": [
                "./*"
            ],
            "@@/*": [
                "./*"
            ]
        },
        "jsx": "preserve",
        "allowJs": true,
    },
    "exclude": [
        "node_modules",
        ".nuxt",
        "dist"
    ],
    "vueCompilerOptions": {
        "target": 2.7
    }
}